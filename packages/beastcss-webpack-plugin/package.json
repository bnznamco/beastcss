{
  "name": "beastcss-webpack-plugin",
  "version": "1.0.11",
  "description": "Webpack plugin to inline critical CSS and async load the rest.",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.esm.js"
  },
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "author": "Freddy Escobar <freddyescobar@hotmail.fr>",
  "keywords": [
    "critical css",
    "inline css",
    "critical",
    "beastcss",
    "webpack plugin",
    "performance",
    "fast"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/freddy38510/beastcss",
    "directory": "packages/beastcss-webpack-plugin"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rimraf dist && rollup -c -w"
  },
  "dependencies": {
    "beastcss": "^1.2.1",
    "micromatch": "4.0.5",
    "schema-utils": "4.0.0"
  },
  "devDependencies": {
    "css-loader": "6.7.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "jsdom": "20.0.0",
    "mini-css-extract-plugin": "2.6.1",
    "webpack": "5.74.0"
  },
  "peerDependencies": {
    "html-webpack-plugin": "^5.0.0",
    "webpack": "^5.21.0"
  },
  "peerDependenciesMeta": {
    "html-webpack-plugin": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
